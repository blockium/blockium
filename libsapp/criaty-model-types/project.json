{
  "name": "criaty-model-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libsapp/criaty-model-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libsapp/criaty-model-types"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libsapp/criaty-model-types"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libsapp/criaty-model-types"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
