{
  "name": "criaty-model",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libsapp/criaty-model/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libsapp/criaty-model"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libsapp/criaty-model"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libsapp/criaty-model"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libsapp/criaty-model/**/*.ts"]
      }
    }
  },
  "tags": []
}
