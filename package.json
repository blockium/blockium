{
  "name": "postgpt",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run start-firebase & (node -e 'setTimeout(()=>{},5000)' && npm run start-app)",
    "start-app": "nx serve app",
    "start-site": "nx serve site",
    "start-firebase": "nx serve firebase-post-gpt",
    "start-zap": "nx serve zapgateway",
    "start-zap-docker": "npm run build-zap-docker && docker run -p 8000:8000 -t zapgateway",
    "build": "npm run build-app && npm run build-site && npm run build-firebase",
    "build-app": "nx build app --configuration production",
    "build-site": "nx build site --configuration production",
    "build-firebase": "nx build firebase-post-gpt --configuration production",
    "build-zap": "nx build zapgateway --configuration production",
    "build-zap-docker": "npx nx docker-build zapgateway",
    "deploy": "npm run build-firebase && nx deploy firebase-post-gpt",
    "deploy-functions": "npm run build-firebase && nx deploy firebase-post-gpt -- --only functions",
    "deploy-hosting": "npm run build-firebase && nx deploy firebase-post-gpt -- --only hosting",
    "deploy-zap": "npm run build-zap && flyctl deploy dist/apps/zapgateway"
  },
  "private": true,
